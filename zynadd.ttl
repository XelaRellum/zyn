@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix llext: <http://ll-plugins.nongnu.org/lv2/ext/>.

<http://home.gna.org/zyn/zynadd/0>
  a lv2:Plugin;
  doap:name "zynadd";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
#  ll:standaloneGui <zynadd_gtk>;
  lv2:optionalHostFeature <http://home.gna.org/lv2dynparam/>;
  ll:pegName "peg";  

  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype llext:miditype;
    lv2:index 0;
    lv2:symbol "midi_in";
    lv2:name "MIDI in";
  ],
  
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "gain";
    lv2:name "Gain";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 0.5;
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "output_left";
    lv2:name "Output Left";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 3;
    lv2:symbol "output_right";
    lv2:name "Output Right";
  ].
